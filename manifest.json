{
  "manifest_version": 2,
  "name": "Teleoka",
  "description" : "Приложение помогает абонентам ООО ТелеОка ком следить за состоянием своего лицевого счета.",
  "version": "1.6.3",
  "icons": { "36": "512.png",
             "48": "512.png",
             "72": "512.png",
             "144": "512.png",
             "192": "512.png",
             "512": "512.png"},
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "http://abonent.teleoka.su/*",
    "storage",
    "notifications",
    "alarms"
  ],
  "browser_action": {
    "default_title": "Телеока ком",
    "default_popup": "popup.html"
  }
}
